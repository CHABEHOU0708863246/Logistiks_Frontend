      "default",
      {
        "link-name": "off",
        "button-name": "off"
      }
    ],
    "disown-opener": "off",
    "button-type": "off",
    "meta-viewport": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter",
          "user-select"
        ]
      }
    ],
    "no-inline-styles": "off",
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off",
        "frame-title": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "select-name": "off",
        "label": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "listitem": "off",
        "list": "off"
      }
    ],
    "axe/aria": [
      "default",
      {
        "aria-progressbar-name": "off",
        "aria-valid-attr-value": "off"
      }
    ],
    "typescript-config/is-valid": "off"
